        "name": "TypeOfCases",
        "version": "2",
        "enumerationValues": [
          {
            "value": "Recovered"
          },
          {
            "value": "Active"
          },
          {
            "value": "Death",
            "synonyms": [
              "Deaths"
            ]
          },
          {
            "value": "Confirmed"
          }
        ],
        "valueSelectionStrategy": "TOP_RESOLUTION"
      }
    ],
    "voiceId": "Justin",
    "childDirected": false,
    "locale": "en-US",
